{
  "name": "github-languages-cli",
  "version": "0.0.0-development",
  "description": "CLI to search for identified languages on GitHub",
  "main": "./build/index.js",
  "bin": {
    "ghlang": "./build/executables/ghlang.js"
  },
  "global": true,
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "babel src/ -d build/ --delete-dir-on-start --copy-files",
    "compile:prod": "BABEL_ENV=production npm run compile",
    "lint": "eslint --ext .js .",
    "test": "jest --coverage --passWithNoTests",
    "prepublishOnly": "npm run compile:prod",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/github-languages-cli.git"
  },
  "keywords": [
    "github",
    "languages",
    "programming languages",
    "cli",
    "github languages",
    "github programming languages"
  ],
  "bugs": {
    "url": "https://github.com/jaebradley/github-languages-cli/issues"
  },
  "homepage": "https://github.com/jaebradley/github-languages-cli#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "github-languages-client": "^1.0.16",
    "inquirer": "^6.0.0",
    "inquirer-autocomplete-prompt": "^0.12.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.0",
    "babel-plugin-add-module-exports": "^0.3.0",
    "babel-preset-minify": "^0.4.0",
    "codecov": "^3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^5.0.0"
  }
}
