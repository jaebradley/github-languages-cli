{
  "env": {
    "production": {
      "presets": [
       [
         "@babel/env",
          { "modules": false },
        ]
      ]
    }
  },
  "plugins": [
    "@babel/transform-runtime",
    "@babel/transform-async-to-generator",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-external-helpers"
  ],
  "ignore": [
    "node_modules/**",
    "*.test.js"
  ]
}
